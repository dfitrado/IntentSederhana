<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/bYoutube"
        android:layout_width="248dp"
        android:layout_height="41dp"
        android:layout_marginTop="36dp"
        android:text="BUKA YOUTUBE"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bGambar" />

    <Button
        android:id="@+id/bGambar"
        android:layout_width="248dp"
        android:layout_height="41dp"
        android:layout_marginTop="36dp"
        android:text="BUKA GAMBAR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bBrowser" />

    <Button
        android:id="@+id/bBrowser"
        android:layout_width="248dp"
        android:layout_height="41dp"
        android:layout_marginTop="40dp"
        android:text="BUKA BROWSER"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bKontak" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/bKontak"
        android:layout_width="248dp"
        android:layout_height="41dp"
        android:layout_marginTop="16dp"
        android:text="BUKA KONTAK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bKamera"
        android:layout_width="247dp"
        android:layout_height="46dp"
        android:layout_marginTop="32dp"
        android:text="BUKA KAMERA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bYoutube" />

    <Button
        android:id="@+id/bMusik"
        android:layout_width="244dp"
        android:layout_height="43dp"
        android:layout_marginTop="36dp"
        android:text="BUKA MUSIK"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bKamera" />

</androidx.constraintlayout.widget.ConstraintLayout>